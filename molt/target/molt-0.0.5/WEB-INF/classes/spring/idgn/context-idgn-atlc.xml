<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean name="atlcNoGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="dataSource" />
        <property name="strategy"   ref="atlcStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="TB_SEQUENCE"/>
        <property name="tableName"  value="ATLC_NO"/>
    </bean>
    <bean name="atlcStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="" />
        <property name="cipers"   value="10" />
        <property name="fillChar" value="0" />
    </bean>
     
    <bean name="certFinishNoGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="dataSource" />
        <property name="strategy"   ref="certFinishNoStrategy" />
        <property name="blockSize"  value="5"/>
        <property name="table"      value="TB_SEQUENCE"/>
        <property name="tableName"  value="CERT_FINISH_NO"/>
    </bean>
    <bean name="certFinishNoStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="" />
        <property name="cipers"   value="5" />
        <property name="fillChar" value="0" />
    </bean> 

</beans>