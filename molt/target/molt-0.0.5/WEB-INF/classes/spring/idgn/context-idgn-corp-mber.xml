<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <!--  롤 ID -->
    <bean name="corpMberGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="dataSource" />
        <property name="strategy"   ref="corpMberIdStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="TB_SEQUENCE"/>
        <property name="tableName"  value="CORP_MBER_ID"/>
    </bean>
    <bean name="corpMberIdStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="B" />
        <property name="cipers"   value="8" />
        <property name="fillChar" value="0" />
    </bean> 
    <!-- 롤 ID -->

</beans>